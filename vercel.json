{
  "outputDirectory": "dist",
  "buildCommand": "npm run vercel-prebuild && npm run build",
  "rewrites": [
    {
      "source": "/group/:groupId",
      "destination": "/"
    },
    {
      "source": "/group/:groupId/(.*)",
      "destination": "/"
    },
    {
      "source": "/activity/:activityId",
      "destination": "/"
    },
    {
      "source": "/activity/:activityId/(.*)",
      "destination": "/"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "redirects": [
    {
      "source": "/privacy.html",
      "destination": "https://www.emojoe.app/privacy.html",
      "permanent": true
    },
    {
      "source": "/terms.html",
      "destination": "https://www.emojoe.app/terms.html",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}