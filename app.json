{
  "expo": {
    "owner": "emojoe",
    "name": "Emojoe",
    "slug": "emojoe2",
    "scheme": "emojoe2",
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./assets/logo_transparent.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logo_transparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/4d6fa508-0b6d-450a-a519-aba3de6dadda"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dk.webcom.emojoe",
      "buildNumber": "1",
      "associatedDomains": ["applinks:emojoe.app", "applinks:www.emojoe.app"],
      "appStoreUrl": "https://apps.apple.com/us/app/emojoe/id1540478554"
    },
    "android": {
      "package": "dk.webcom.emojoe",
      "versionCode": 1,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_transparent.png",
        "backgroundColor": "#fff"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "webcomdk",
            "project": "emojoe2"
          }
        }
      ]
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "plugins": ["sentry-expo"],
    "extra": {
      "eas": {
        "projectId": "4d6fa508-0b6d-450a-a519-aba3de6dadda"
      }
    }
  }
}
