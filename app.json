{
  "expo": {
    "owner": "emojoe",
    "name": "Emojoe",
    "slug": "emojoe",
    "scheme": "emojoe",
    "version": "0.8.0.8",
    "orientation": "portrait",
    "entryPoint": "./src/root.ts",
    "icon": "./assets/logo_transparent.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logo_transparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/4b6d8773-7a99-4388-9b58-f658be614e3b"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dk.webcom.emojoe",
      "buildNumber": "1"
    },
    "android": {
      "package": "dk.webcom.emojoe",
      "versionCode": 1,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_transparent.png",
        "backgroundColor": "#fff"
      }
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "",
            "project": "",
            "authToken": ""
          }
        }
      ]
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "extra": {
      "eas": {
        "projectId": "4b6d8773-7a99-4388-9b58-f658be614e3b"
      }
    }
  }
}
