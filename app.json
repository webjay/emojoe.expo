{
  "expo": {
    "owner": "emojoe",
    "name": "Emojoe",
    "slug": "emojoe2",
    "scheme": "emojoe2",
    "version": "2.2.1",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dk.webcom.emojoe",
      "buildNumber": "1",
      "associatedDomains": [
        "applinks:emojoe.app"
      ],
      "appStoreUrl": "https://apps.apple.com/app/emojoe/id1540478554",
      "entitlements": {
        "com.apple.developer.applesignin": [
          "Default"
        ]
      }
    },
    "android": {
      "package": "dk.webcom.emojoe",
      "versionCode": 11,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_transparent_1000.png",
        "backgroundColor": "#fff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "emojoe.app",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "name": "Emojoe",
      "description": "Build a habit"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "webcomdk",
            "project": "emojoe2"
          }
        }
      ]
    },
    "plugins": [
      "sentry-expo"
    ],
    "extra": {
      "eas": {
        "projectId": "4d6fa508-0b6d-450a-a519-aba3de6dadda"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/4d6fa508-0b6d-450a-a519-aba3de6dadda"
    }
  }
}
